===================================install Q7 ubuntu 過程,後捨棄======================================================

1. sudo mkfs.ext4 -L Q7_3.14.28 /dev/sdb1
2. sudo tar pxvf ubuntu-14.04-hf-minimal-20151015.tgz -C /media/rtx-3110/Q7_3.14.28/
3. sudo cp -avrf lib /media/rtx-3110/Q7_3.14.28/
4. sudo cp -avrf include /media/rtx-3110/Q7_3.14.28/usr/
5. sudo vi /media/rtx-3110/Q7_3.14.28/etc/fstab
6. sudo cp -avrf imx6 misc /media/rtx-3110/Q7_3.14.28/home/rtx/
	--------- console ------
7. apt-get update
8. apt-get upgrade

9. imx-lib-3.14.28-1.0.0
	a. tar -zxvf imx-lib-3.14.28-1.0.0.tar.gz
	b. make -j1 PLATFORM=IMX6Q all
		- apt-get install g++
	c. make install
		- modify rng/ screenlayer/ sim/ Makefile don't install

10. firmware-imx-3.14.28
	a. chmod +x firmware-imx-3.14.28-1.0.0.bin
	b. ./firmware-imx-3.14.28-1.0.0.bin (Unpacking file)
	c. cp -avrf firmware/* /lib/firmware/

11. libfslcodec-4.0.3
	a. chmod +x libfslcodec-4.0.3.bin
	b. ./libfslcodec-4.0.3.bin (Unpacking file)
	c. ./configure
	d. ./autogen.sh
		- apt-get install autoconf
		- apt-get install make
	e. ./autogen.sh --prefix=/usr && make all && sudo make DESTDIR=/ install

12. libfslparser-4.0.3
	a. chmod +x libfslparser-4.0.3.bin
	b. ./libfslparser-4.0.3.bin (Unpacking file)
	c. ./autogen.sh --prefix=/usr && make all && sudo make DESTDIR=/ install

13. imx-vpu-5.4.28
	a. chmod +x imx-vpu-5.4.28.bin
	b. ./imx-vpu-5.4.28.bin
	c. make -j1 PLATFORM=IMX6Q all
	d. make install

14. libfslvpuwrap-1.0.58
	a. chmod +x libfslvpuwrap-1.0.58.bin
	b. ./libfslvpuwrap-1.0.58.bin
	c. ./autogen.sh --prefix=/usr && make all && sudo make DESTDIR=/ install
		- apt-get install libtool

15. gstreamer-0.10.36
	a. tar jxvf gstreamer-0.10.36.tar.bz2
	b. ./autogen.sh --prefix=/usr && make all && sudo make DESTDIR=/ install
		- apt-get install autopoint
		- apt-get install pkg-config
