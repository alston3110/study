	c. ./configure --prefix=/usr --sysconfdir=/etc --with-gtk=3.0 --with-package-name="GStreamer Good Plugins 0.10.31 BLFS" --with-package-origin="http://www.linuxfromscratch.org/blfs/view/systemd/"
		- apt-get install liborc-0.4-dev
	d. make
	e. make install


